<%= form_for [:admin, @store, @product_store] do |f| %>

    <label>Product:</label>
    <%= f.label(@product_store.product.name)%>
    <br>
    <label>Amount:</label>
    <%= f.text_field(:amount) %>
    <br>

    <%= f.submit 'Save' %>

<% end %>

<%= link_to 'Back', admin_store_product_stores_path %>
